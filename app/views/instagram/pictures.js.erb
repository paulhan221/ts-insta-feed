$("<%= escape_javascript(render 'ajaxthumbnail') %>").appendTo(".instagram-container");

var $container = $('.instagram-container');

$container.masonry('reloadItems');

$(function(){
  $container.imagesLoaded(function(){
    $container.masonry({
      itemSelector : '.thumbnail',
      columnWidth: 20,
      isAnimated: true
    });
  });
});

